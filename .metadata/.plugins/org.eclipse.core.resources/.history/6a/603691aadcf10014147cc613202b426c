
public class LinkedList {
	
	private class Node {
		int data;
		Node next;
		
		public Node(int data){
			this.data = data;
			this.next = null;
		}
		
		public Node(int data, Node nextVal){
			next =  nextVal;
			this.data = data;
		}
		
		public int getData(){
			return this.data;
		}
		
		public Node getNext(){
			return this.next;
		}
		
		public void setData(int data){
			this.data = data;
		}
		
		
		
	}

}
